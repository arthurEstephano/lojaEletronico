<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.edu.femass.controller.ProdutoController">
    <children>
        <Pane minHeight="356.0" minWidth="225.0" prefHeight="356.0" prefWidth="225.0">
            <children>
                <Button fx:id="BtnIncluir" layoutX="14.0" layoutY="246.0" mnemonicParsing="false" onAction="#BtnIncluir_Action" prefHeight="36.0" prefWidth="92.0" text="Incluir" />
            <ChoiceBox fx:id="CbxProduto" layoutX="91.0" layoutY="34.0" prefHeight="25.0" prefWidth="189.0" />
            <Text layoutX="11.0" layoutY="51.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Produto" />
            <Text layoutX="11.0" layoutY="102.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Quantidade" />
            <TextField fx:id="TxtQtd" layoutX="91.0" layoutY="85.0" prefHeight="25.0" prefWidth="92.0" />
            <ChoiceBox fx:id="CbxUsuario" layoutX="91.0" layoutY="184.0" prefHeight="25.0" prefWidth="189.0" />
            <Text layoutX="14.0" layoutY="201.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Fornecedor" />
            <Text layoutX="11.0" layoutY="155.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Valor" />
            <TextField fx:id="TxtValor" layoutX="91.0" layoutY="138.0" prefHeight="25.0" prefWidth="92.0" />
            </children>
        </Pane>
        <Pane minHeight="356.0" minWidth="316.0" prefHeight="365.0" prefWidth="346.0">
            <children>
                <Button fx:id="BtnGravar" disable="true" layoutX="231.0" layoutY="304.0" mnemonicParsing="false" onAction="#BtnGravar_Action" prefHeight="36.0" prefWidth="92.0" text="Comprar" />
                <Button fx:id="BtnCancelar" disable="true" layoutX="77.0" layoutY="304.0" mnemonicParsing="false" onAction="#BtnCancelar_Action" prefHeight="36.0" prefWidth="92.0" text="Cancelar" />
                <ListView fx:id="LstProdutos" layoutX="77.0" layoutY="14.0" onKeyPressed="#LstProdutos_KeyPressed" onMouseClicked="#LstProdutos_MouseClicked" prefHeight="287.0" prefWidth="246.0" />
            </children>
        </Pane>
    </children>
</HBox>
